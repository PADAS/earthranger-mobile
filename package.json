{
  "name": "das_mobile_rn",
  "version": "2.6.1",
  "private": true,
  "scripts": {
    "preinstall": "git config core.hooksPath .githooks",
    "postinstall": "patch-package && bash patches/update-mmkv.sh",
    "check-dependencies": "rnx-align-deps",
    "fix-dependencies": "rnx-align-deps --write",
    "android": "react-native run-android --appId=com.earthranger.debug",
    "android-build": "cd android && ./gradlew clean && ./gradlew assembleDebug --info && cd ../",
    "android-clean": "cd android && ./gradlew clean && cd ../",
    "android-beta": "cd android && ./gradlew clean bundleBeta && cd ../",
    "android-pull-db": "adb shell \"run-as com.earthranger.debug cat /data/data/com.earthranger.debug/databases/earthranger.db\" > ./build/android/earthranger.db",
    "android-release": "cd android && ./gradlew clean bundleRelease && cd ../",
    "android-logcat": "adb logcat -v color -d | grep -F \"`adb shell ps | grep com.earthranger  | tr -s [:space:] ' ' | cut -d' ' -f2`\"",
    "ios": "react-native run-ios --device $npm_config_device",
    "ios-clean": "cd ios && xcodebuild clean -workspace EarthRanger.xcworkspace -scheme EarthRanger && cd ../",
    "ios-pull-db": "ios-deploy --download=/Library/LocalDatabase --bundle_id 'com.earthranger.debug' --to ./build/ios",
    "mock-list": "mockoon-cli list",
    "mock-start": "mockoon-cli start --data mock/earthranger.json",
    "mock-stop": "mockoon-cli stop mockoon-earthranger",
    "refresh-deps": "yarn && cd ios && pod install && cd ../",
    "remove-deps": "rm -rf node_modules && cd ios && rm -rf Pods && cd ../",
    "start": "react-native start",
    "test": "jest src/.*\\.test\\..* --coverage",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@earthranger/react-native-jsonforms-formatter": "^1.0.1",
    "@gorhom/bottom-sheet": "^4.4.6",
    "@jsonforms/core": "3.0.0",
    "@jsonforms/react": "3.0.0",
    "@react-native-community/checkbox": "^0.5.9",
    "@react-native-community/clipboard": "^1.5.1",
    "@react-native-community/datetimepicker": "^6.3.2",
    "@react-native-community/netinfo": "^8.0.0",
    "@react-native-firebase/analytics": "19.3.0",
    "@react-native-firebase/app": "19.3.0",
    "@react-native-firebase/crashlytics": "19.3.0",
    "@react-native-material/core": "^1.3.7",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/material-top-tabs": "^6.0.6",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.2.5",
    "@rnmapbox/maps": "10.1.28",
    "@shopify/flash-list": "^1.6.3",
    "@tsconfig/react-native": "2.0.2",
    "@turf/area": "^6.5.0",
    "@turf/bbox": "^6.5.0",
    "@turf/distance": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "@turf/length": "^6.5.0",
    "@turf/polygon-to-line": "^6.5.0",
    "@types/geodesy": "^2.2.3",
    "@types/react": "^18.2.6",
    "axios": "^0.24.0",
    "buffer": "^6.0.3",
    "dayjs": "^1.11.2",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eventemitter3": "^5.0.0",
    "formik": "^2.2.9",
    "geodesy": "^2.4.0",
    "i18next": "^21.6.0",
    "lodash-es": "^4.17.21",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "2.4.1",
    "react": "18.2.0",
    "react-devtools": "^4.24.4",
    "react-devtools-core": "^4.24.4",
    "react-i18next": "^11.15.1",
    "react-native": "0.69.7",
    "react-native-awesome-pin": "^1.3.4",
    "react-native-background-fetch": "^4.2.5",
    "react-native-background-geolocation": "^4.16.3",
    "react-native-config": "^1.4.5",
    "react-native-device-info": "^8.4.8",
    "react-native-fs": "^2.18.0",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-get-random-values": "^1.8.0",
    "react-native-image-picker": "^7.1.2",
    "react-native-image-resizer": "^1.4.5",
    "react-native-immersive-mode": "^2.0.1",
    "react-native-localize": "^2.2.2",
    "react-native-logs": "^4.0.1",
    "react-native-mail": "file:Frameworks/react-native-mail/",
    "react-native-mmkv": "2.5.1",
    "react-native-modal-datetime-picker": "^14.0.0",
    "react-native-pager-view": "^5.4.9",
    "react-native-paper": "^4.11.2",
    "react-native-reanimated": "^2.12.0",
    "react-native-safe-area-context": "^4.2.5",
    "react-native-screens": "^3.13.1",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-svg": "^12.3.0",
    "react-native-tab-view": "^3.1.1",
    "react-native-ui-lib": "^6.20.3",
    "react-native-vector-icons": "^9.0.0",
    "react-native-zip-archive": "^6.0.6",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.0",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-transform-react-jsx": "^7.20.13",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@babel/runtime": "^7.20.7",
    "@react-native-community/eslint-config": "^3.0.1",
    "@rnx-kit/align-deps": "^2.5.0",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-native": "^9.0.0",
    "@types/jest": "^29.2.1",
    "@types/lodash-es": "^4.17.6",
    "@types/moxios": "^0.4.15",
    "@types/react-native": "^0.66.8",
    "@types/react-native-sqlite-storage": "^5.0.2",
    "@types/react-test-renderer": "^18.0.0",
    "@types/redux-mock-store": "^1.0.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jest": "^29.2.1",
    "jest-fetch-mock": "^3.0.3",
    "metro": "^0.80.9",
    "metro-react-native-babel-preset": "^0.77.0",
    "moxios": "^0.4.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "18.2.0",
    "redux-mock-store": "^1.5.4",
    "typescript": "4.8.4"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^lodash-es$": "lodash"
    },
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!<rootDir>/src/common/constants/*.{ts,tsx}",
      "!<rootDir>/src/common/mockData/*.{ts,tsx}",
      "!**/*.styles.ts",
      "!**/styles.ts",
      "!**/*.png"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@?react-native|@react-native-community|react-navigation)"
    ]
  },
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "requirements": [
        "react-native@0.68"
      ],
      "capabilities": [
        "animation",
        "babel-preset-react-native",
        "checkbox",
        "core",
        "core-android",
        "core-ios",
        "datetime-picker",
        "filesystem",
        "gestures",
        "jest",
        "metro",
        "navigation/native",
        "netinfo",
        "react",
        "react-test-renderer",
        "safe-area",
        "screens",
        "sqlite",
        "svg"
      ]
    }
  }
}